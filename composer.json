{
    "name": "mingzaily/lumen-permission",
    "description": "Permission handling for lumen 6.x and up",
    "keywords": [
        "mingzaily",
        "lumen",
        "permission",
        "permissions",
        "roles",
        "rbac",
        "security"
    ],
    "homepage": "https://github.com/mingzaily/lumen-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "mingzaily",
            "email": "mingzaily@163.com",
            "homepage": "https://www.mingzaily.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/auth": "^6.0|^7.0",
        "illuminate/container": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.4",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Mingzaily\\Permission\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mingzaily\\Permission\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mingzaily\\Permission\\PermissionServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
